{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "BasicVerison",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/BasicVersion/RBE-C++/build/${fileBasenameNoExtension}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "${workspaceFolder}/mcl-lib/lib:${env:LD_LIBRARY_PATH}"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "BasicVersion(C++): g++ build active file"
        },
        {
